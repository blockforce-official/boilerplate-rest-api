{
  "name": "blockforce-rest-api",
  "version": "1.0.0",
  "description": "Blockforce REST API boilerplate for new projetcts",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockforce-official/boilerplate-rest-api#license"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "dev:local": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "dev:docker": "ts-node-dev --inspect=0.0.0.0:9229 --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "db:migrations:make": "knex --knexfile src/app/config/knexfile.ts migrate:make -x ts",
    "db:migrate": "knex --knexfile src/app/config/knexfile.ts migrate:latest",
    "db:rollback:last": "knex --knexfile src/app/config/knexfile.ts migrate:down",
    "db:rollback:all": "knex --knexfile src/app/config/knexfile.ts migrate:rollback --all",
    "db:seeds:make": "knex --knexfile src/app/config/knexfile.ts seed:make -x ts",
    "db:seeds:run": "knex --knexfile src/app/config/knexfile.ts seed:run",
    "start": "node ./build/index.js",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.95.6",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^27.0.3",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
}
